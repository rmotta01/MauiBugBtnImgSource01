<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1">

    <ShellContent
        Title="HorizontalStackPanel"
        ContentTemplate="{DataTemplate local:HorizStackPnl}"
        Route="HorizStackPnl" />

    <ShellContent
        Title="GridColAuto"
        ContentTemplate="{DataTemplate local:GridColAuto}"
        Route="GridColAuto" />

    <ShellContent
        Title="Flex"
        ContentTemplate="{DataTemplate local:Flex}"
        Route="Flex" />

</Shell>
